{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@webcobra/structures",
  "version": "0.1.0",
  "license": "MIT",
  "ngPackage": {
    "lib": {
      "entryFile": "src/public-api.ts"
    }
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "api-extract": "api-extractor run --local --verbose",
    "generate-docs": "api-documenter markdown -i api -o docs",
    "postbuild": "yarn api-extract && yarn generate-docs",
    "test": "jest",
    "coverage": "jest --ci --reporters=jest-junit --coverage --coverageReporters=cobertura",
    "lint": "eslint . --ext .js,.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@angular/common": "^9.1.9",
    "@angular/core": "^11.0.5",
    "@angular/platform-browser": "^9.1.9",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/core": "^11.0.5",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@microsoft/api-documenter": "^7.7.12",
    "@microsoft/api-extractor": "^7.7.8",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "jest": ">=25 <26",
    "jest-junit": "^10.0.0",
    "jest-preset-angular": "^8.1.0",
    "ng-packagr": "^10.1.1",
    "rxjs": "^6.5.3",
    "ts-jest": "^25.0.0",
    "tslib": "^1.10.0",
    "typescript": ">=3.6 <3.8",
    "zone.js": "^0.10.2"
  }
}
